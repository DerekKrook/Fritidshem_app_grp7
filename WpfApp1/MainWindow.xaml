<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="FirstPage" Style="{StaticResource MyWindowStyle}">
    <Grid>
        <Button x:Name="btnGuardian" Content="Vårdnadshavare" HorizontalAlignment="Left" Margin="100,128,0,0" VerticalAlignment="Top" Width="200" Height="120" Click="BtnGuardian_Click" Style="{StaticResource ButtonStyleNormal}"/>
        <Button x:Name="btnStaff" Content="Personal" HorizontalAlignment="Left" Margin="500,128,0,0" VerticalAlignment="Top" Width="200" Height="120" Click="BtnStaff_Click" Style="{StaticResource ButtonStyleNormal}"/>

    </Grid>
</Window>
